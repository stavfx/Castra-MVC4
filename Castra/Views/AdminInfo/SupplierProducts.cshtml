@model IEnumerable<Castra.Controllers.SupplierProducts>
@{
    ViewBag.Title = "SupplierProducts";
}

<h2>@ViewBag.Title</h2>

<table>
    <tr>
        <th>Supplier</th>
        <th>Products</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.ProductList</td>
        </tr>
    }
</table>